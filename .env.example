# OnchainKit API Key (required)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key_here

# CDP SDK Configuration (optional - for server-side wallet operations)
CDP_API_KEY_ID=your_cdp_api_key_id
CDP_API_KEY_SECRET=your_cdp_api_key_secret
CDP_WALLET_SECRET=your_cdp_wallet_secret

# x402 Configuration (optional - uses default facilitator if not set)
NEXT_PUBLIC_X402_FACILITATOR_URL=https://facilitator.x402.org

# Development Settings
NODE_ENV=development
